void keyPressed() { 
  switch(keyCode) {

    // v  
    case 86:
      next(1);
      break;
      
    // b 
    case 66:
      next(2);
      break;
      
    // n  
    case 78:
      next(3);
      break;
      
    // m  
    case 77:
      songEnd(1);
      break;

  
  }
}
